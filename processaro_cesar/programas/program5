MOV R0,128(ENDERECO INICIAL DO VETOR)
MOV R1,129(32 = TAMANHO DO VETOR)
MOV R2,ENDERECO INICIAL DO VETOR(DEPOIS O MAIOR VALOR ENCONTRADO)
MOV R4, QTD DE INSTRUCOES CASO O VALOR ATUAL NAO SEJA MAIOR = 4
MOV R5, QTD DE INSTRUCOES CASO O VALOR ATUAL SEJA MAIOR = -3
INC R0
MOV R3,(R0)(PROXIMO VALOR DO VETOR)
SUB R3,R2
BMI R4
DEC R1
BNE R5
MOV R2,(R0) //PODE SER TBM MOV DE REG. PARA REG. QUANDO IMPLEMENTADO
DEC R1
BNE R5


